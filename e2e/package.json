    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.6",
    "system": "^2.0.1",
    "test-data-bot": "^0.8.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5"
  }
