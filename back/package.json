{
  "name": "c3pm-api",
  "scripts": {
    "start": "node dist/",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc --project tsconfig.build.json",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env NODE_ENV=development ts-node --transpile-only src/graphql/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/",
    "test": "npm run -s generate:nexus && jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint . --ext .ts --ignore-path=.gitignore",
    "lint:fix": "eslint . --ext .ts --fix --ignore-path=.gitignore",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.5.0",
    "@sendgrid/mail": "^7.4.0",
    "apollo-server-express": "^2.19.0",
    "aws-sdk": "^2.828.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "env-var": "^7.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "form-data": "^3.0.0",
    "graphql": "^15.4.0",
    "graphql-iso-date": "^3.6.1",
    "multer": "^1.4.2",
    "nexus": "^1.0.0",
    "node-fetch": "^2.6.1",
    "semver": "^7.3.2",
    "tar": "^6.0.5",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "yaml": "^1.10.0",
    "yup": "^0.31.0"
  },
  "devDependencies": {
    "@types/express-session": "^1.17.3",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "^26.0.15",
    "@types/multer": "^1.4.4",
    "@types/node": "14.14.10",
    "@types/semver": "^7.3.4",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.1.0",
    "fetch-cookie": "^0.11.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "nanoid": "^3.1.18",
    "pg": "^8.5.1",
    "prisma": "^3.5.0",
    "ts-jest": "^26.4.4",
    "ts-node": "9.0.0",
    "ts-node-dev": "1.0.0",
    "typescript": "4.1.2"
  },
  "engines": {
    "node": "12.x"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  }
}
